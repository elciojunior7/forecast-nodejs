{
  "name": "forecast-nodejs",
  "version": "1.0.0",
  "description": "Typescript node.js API\u001b[D\u001b[D\u001b[DForecast API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test": "npm run test:unit && npm run test:functional"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elciojunior7/forecast-nodejs.git"
  },
  "keywords": [
    "forecast",
    "nodejs",
    "typescript"
  ],
  "author": "Elcio Jr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elciojunior7/forecast-nodejs/issues"
  },
  "homepage": "https://github.com/elciojunior7/forecast-nodejs#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.13",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.2.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "0.0.41",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "16",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "express": "^4.17.2",
    "jest": "^27.4.5",
    "nock": "^13.2.4",
    "prettier": "^2.6.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
